{
  "name": "backmedcenter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "watch": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc && tsc-alias",
    "generate": "npm run build && npx typeorm migration:generate ./src/db/migrations/$npm_config_name -d ./dist/db/data-source.js && npm run build",
    "migration": "npx typeorm migration:run -d dist/db/data-source.js",
    "swagger:gen": "node dist/utils/other/swagger.js",
    "lint": "eslint .",
    "prettier": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Calm-the-loony/BackMedCenter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Calm-the-loony/BackMedCenter/issues"
  },
  "homepage": "https://github.com/Calm-the-loony/BackMedCenter#readme",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jiti": "^2.6.1",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bcrypt": "^6.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.6",
    "@types/glob": "^8.1.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/redis": "^4.0.10",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "prettier": "3.7.4",
    "tsc-alias": "^1.8.16",
    "typescript-eslint": "^8.50.1"
  }
}
